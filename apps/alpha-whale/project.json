{
  "name": "alpha-whale",
  "sourceRoot": "apps/alpha-whale/agent",
  "projectType": "application",
  "tags": ["scope:alpha-whale", "lang:python"],
  "implicitDependencies": ["py-core"],
  "targets": {
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "poetry run pytest apps/alpha-whale/tests apps/alpha-whale/api/tests -v",
        "cwd": "."
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "poetry run ruff check apps/alpha-whale/agent apps/alpha-whale/ingestion apps/alpha-whale/api",
        "cwd": "."
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "poetry run mypy apps/alpha-whale/agent apps/alpha-whale/ingestion apps/alpha-whale/api",
        "cwd": "."
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "poetry run uvicorn api.main:app --reload",
        "cwd": "apps/alpha-whale"
      }
    }
  }
}
